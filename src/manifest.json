{
  "name": "Lemirarma",
  "version": "0.1",
  "description": "Lemirarma",
  "permissions": [
    "tabs", "alarms", "storage", "https://www.google.com/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
      "default_title": "Lemirarma",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
